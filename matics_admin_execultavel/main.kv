<Home_Screen>:
    name:'home'
    MDBoxLayout:
        orientation: "vertical"
        MDBoxLayout:
            MDFloatLayout:
                MDLabel:
                    text: 'Escolha o Assunto!'
                    font_name: 'Roboto-Bold'
                    pos_hint: {'center_x': 0.8, 'center_y': 0.8}
                    color: "#0C0756"
                    font_size: 48

                MDLabel:
                    padding: 100, 20
                    text: 'Pontos'
                    font_name: 'Roboto-Bold'
                    pos_hint: {'center_y': 0.57, 'center_x': .8}
                    color: "#EE8F00"
                    font_size: 34
                MDFloatLayout:
                    id: Pontos
                    MDIconButton:
                        icon: "eye"
                        id: icon_olho
                        pos_hint: {'center_y': 2.55, 'center_x': .54}
                        theme_text_color: "Custom"
                        text_color: "#EE8F00"
                        on_release:
                            self.icon = "eye" if self.icon == "" else ""
                            root.pontos() if self.icon == "" else root.remove()

                MDFillRoundFlatButton:
                    text: "Cadastrar Juros Simples"
                    font_name: 'Roboto-Bold'
                    font_size: 24
                    text_color: "white"
                    md_bg_color: "#EC5952"
                    size_hint: .25, .07
                    pos_hint: {"center_x": .17 ,"center_y": .4}
                    on_release:
                        app.root.current = "cadastrar_simples"

                MDFillRoundFlatButton:
                    text: "Cadastrar Juros Composto"
                    font_name: 'Roboto-Bold'
                    font_size: 24
                    text_color: "white"
                    md_bg_color: "#EC5952"
                    size_hint: .25, .07
                    pos_hint: {"center_x": .83 ,"center_y": .4}
                    on_release:
                        app.root.current = "cadastrar_composto"

                MDFillRoundFlatButton:
                    text: "Lista de Exercicios"
                    font_name: 'Roboto-Bold'
                    font_size: 24
                    text_color: "white"
                    md_bg_color: "#EC5952"
                    size_hint: .25, .07
                    pos_hint: {"center_x": .17 ,"center_y": .65}
                    on_release:
                        root.lista_simples()

                MDFillRoundFlatButton:
                    text: "Lista de Exercicios"
                    font_name: 'Roboto-Bold'
                    font_size: 24
                    text_color: "white"
                    md_bg_color: "#EC5952"
                    size_hint: .25, .07
                    pos_hint: {"center_x": .83 ,"center_y": .65}
                    on_release:
                        root.lista_compostos()

        MDBoxLayout:
            MDFloatLayout:
                md_bg_color: "#EC5952"
                Image:
                    source: 'assets/juros_compostos.jpg'
                    size_hint: None, None
                    size: 350, 350
                    pos_hint: {'center_x':.5, 'center_y': .69}

            MDFloatLayout:

            MDFloatLayout:
                md_bg_color: "#EC5952"
                Image:
                    pos_hint_x: "right"
                    source: 'assets/juros_simples.jpeg'
                    size_hint: 1, 1
                    pos_hint: {'center_x':.5, 'center_y': .69}

        MDBoxLayout:
            MDFloatLayout:
                id: simples
                md_bg_color: "#EC5952"
                MDRaisedButton:
                    text: "Exercícios"
                    font_name: 'Roboto-Bold'
                    font_size: 24
                    text_color: "#EC5952"
                    md_bg_color: "white"
                    size_hint: .7, .06
                    pos_hint: {"center_x": .5 ,"center_y": .6}
                    on_release:
                        root.mudar_icon()
                        root.exercicio_simples()

                MDRaisedButton:
                    text: "Estudar"
                    font_name: 'Roboto-Bold'
                    font_size: 24
                    text_color: "#EC5952"
                    md_bg_color: "white"
                    size_hint: .7, .06
                    pos_hint: {"center_x": .5 ,"center_y": .3}
                    on_release:
                        root.mudar_icon()
                        app.root.current = "estudar_simples"

                MDLabel:
                    text: 'Juros Simples'
                    font_name: 'Roboto-Bold'
                    halign: 'center'
                    pos_hint: {'center_y': .95}
                    color: "white"
                    font_size: 35

            MDFloatLayout:

            MDFloatLayout:
                id: compostos
                md_bg_color: "#EC5952"
                MDRaisedButton:
                    text: "Exercícios"
                    font_name: 'Roboto-Bold'
                    font_size: 24
                    text_color: "#EC5952"
                    md_bg_color: "white"
                    size_hint: .7, .06
                    pos_hint: {"center_x": .5 ,"center_y": .6}
                    on_release:
                        root.mudar_icon()
                        root.exercicio_composto()

                MDRaisedButton:
                    text: "Estudar"
                    font_name: 'Roboto-Bold'
                    font_size: 24
                    text_color: "#EC5952"
                    md_bg_color: "white"
                    size_hint: .7, .06
                    pos_hint: {"center_x": .5 ,"center_y": .3}
                    on_release:
                        root.mudar_icon()
                        app.root.current = "estudar_compostos"

                MDLabel:
                    text: 'Juros Compostos'
                    font_name: 'Roboto-Bold'
                    pos_hint: {'center_y': .95, 'center_x': .6}
                    color: "white"
                    font_size: 35

    MDFloatLayout:
        id: tela_zerado
        pos_hint: {'center_y': 10}
        md_bg_color: "white"
        MDLabel:
            text: 'OPS, Parece que seus pontos estão zerados!\nTente revisar o assunto para conseguir jogar novamente!\n\n Você pode assistir videos na página de estudos!'
            font_name: 'Roboto-Bold'
            halign: "center"
            pos_hint: {'center_y': 0.6}
            color: "#EC5952"
            font_size: 40
            padding: 80, 40

        MDFillRoundFlatButton:
            text: "Voltar"
            font_name: 'Roboto-Bold'
            font_size: 24
            text_color: "white"
            md_bg_color: "#EC5952"
            size_hint: .15, .06
            pos_hint: {"center_x": .5 ,"center_y": .1}
            on_release:
                root.mudar_icon()
                root.voltar_home()

    MDBoxLayout:
        id: lista
        md_bg_color: "white"
        pos_hint: {"center_y": 10}
        orientation: "vertical"
        MDTopAppBar:
            title: "Matics"
            md_bg_color: "#EC5952"
            elevation: 0
            left_action_items:
                [["arrow-left", lambda x: root.sair_lista()]]
        MDFloatLayout:
            id: lista_users

    MDFloatLayout:
        id: exercicio_simp
        pos_hint: {'center_y': 10}
        md_bg_color: "white"
        MDLabel:
            id: dentro_questao_simp
            font_name: 'Roboto-Bold'
            halign: "center"
            pos_hint: {'center_y': 0.66}
            color: "#EC5952"
            font_size: 24
            padding: 80, 40

        MDLabel:
            id: respostass
            padding: 80, 20
            font_name: 'Roboto-Bold'
            halign: 'center'
            pos_hint: {'center_y': 0.3}
            color: "#EC5952"
            font_size: 34

        MDFillRoundFlatButton:
            text: "Deletar"
            font_name: 'Roboto-Bold'
            font_size: 24
            text_color: "white"
            md_bg_color: "#EC5952"
            size_hint: .15, .06
            pos_hint: {"center_x": .83 ,"center_y": .07}
            on_release:
                root.excluir_questao_simples(root.questao["_id"])

        MDFillRoundFlatButton:
            text: "Voltar"
            font_name: 'Roboto-Bold'
            font_size: 24
            text_color: "white"
            md_bg_color: "#EC5952"
            size_hint: .15, .06
            pos_hint: {"center_x": .12 ,"center_y": .07}
            on_release:
                root.sair_dentro_questao_simples()

    MDFloatLayout:
        id: exercicio_comp
        pos_hint: {'center_y': 10}
        md_bg_color: "white"
        MDLabel:
            id: dentro_questao_comp
            font_name: 'Roboto-Bold'
            halign: "center"
            pos_hint: {'center_y': 0.66}
            color: "#EC5952"
            font_size: 24
            padding: 80, 40

        MDLabel:
            id: respostasc
            padding: 80, 20
            font_name: 'Roboto-Bold'
            halign: 'center'
            pos_hint: {'center_y': 0.3}
            color: "#EC5952"
            font_size: 34

        MDFillRoundFlatButton:
            text: "Deletar"
            font_name: 'Roboto-Bold'
            font_size: 24
            text_color: "white"
            md_bg_color: "#EC5952"
            size_hint: .15, .06
            pos_hint: {"center_x": .83 ,"center_y": .07}
            on_release:
                root.excluir_questao_compostos(root.questao["_id"])

        MDFillRoundFlatButton:
            text: "Voltar"
            font_name: 'Roboto-Bold'
            font_size: 24
            text_color: "white"
            md_bg_color: "#EC5952"
            size_hint: .15, .06
            pos_hint: {"center_x": .12 ,"center_y": .07}
            on_release:
                root.sair_dentro_questao_compostos()


<Estudar_Simples_Screen>:
    name: "estudar_simples"
    MDFloatLayout:
        md_bg_color: "#EC5952"
        MDLabel:
            text: 'Juros Simples'
            font_name: 'Roboto-Bold'
            halign: 'center'
            pos_hint: {'center_y': 0.9}
            color: "white"
            font_size: 54

        MDLabel:
            text: 'O que é?'
            font_name: 'Roboto-Bold'
            halign: 'center'
            pos_hint: {'center_y': 0.7}
            color: "white"
            font_size: 42

        MDLabel:
            padding: 20, 40
            text: 'Os juros simples são correções feitas em um valor aplicado ou devido. Os juros são calculados a partir de uma porcentagem preestabelecida e leva em consideração o período da aplicação ou da dívida. Um valor aplicado é chamado de capital, já a porcentagem de correção é chamada de taxa de juros.'
            font_name: 'Roboto-Bold'
            halign: 'center'
            pos_hint: {'center_y': 0.45}
            color: "white"
            font_size: 24

        MDFillRoundFlatButton:
            text: "Fórmula"
            font_name: 'Roboto-Bold'
            font_size: 24
            text_color: "#EC5952"
            md_bg_color: "white"
            size_hint: .25, .06
            pos_hint: {"center_x": .83 ,"center_y": .07}
            on_release:
                app.root.current = "formula_simples"


        MDFillRoundFlatButton:
            text: "Voltar"
            font_name: 'Roboto-Bold'
            font_size: 24
            text_color: "#EC5952"
            md_bg_color: "white"
            size_hint: .15, .06
            pos_hint: {"center_x": .12 ,"center_y": .07}
            on_release:
                app.root.current = "home"


