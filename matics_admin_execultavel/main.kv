<Login_Screen>:
    name:'login'
    MDBoxLayout:
        orientation: "horizontal"
        MDFloatLayout:
            id: Login
            md_bg_color: "#EC5952"
            Image:
                source: 'assets/user_azul.png'
                size_hint: None, None
                size: 165, 165
                pos_hint: {'center_x':.5, 'center_y': .8}

            MDLabel:
                text: 'Bem Vindo!'
                font_name: 'Roboto-Bold'
                halign: 'center'
                pos_hint: {'center_y': 0.65}
                color: "white"
                font_size: 48

            MDTextField:
                id: email
                hint_text: "E-mail"
                hint_text_color_focus: "white"
                pos_hint: {'center_y': 0.5, 'center_x': 0.45}
                size_hint: .7, .1
                on_hint_text_color_normal: "white"
                text_color_focus: "white"

            MDTextField:
                id: senha
                hint_text: "Senha"
                hint_text_color_focus: "white"
                pos_hint: {'center_y': 0.4, 'center_x': 0.45}
                size_hint: .7, .1
                on_hint_text_color_normal: "white"
                text_color_focus: "white"

            MDFillRoundFlatButton:
                text: "Entrar"
                font_name: 'Roboto-Bold'
                font_size: 24
                text_color: "white"
                md_bg_color: "#3485A7"
                size_hint: .7, .06
                pos_hint: {"center_x": .5 ,"center_y": .23}
                on_release:
                    root.login()

            MDLabel:
                text: 'Ainda não tem conta?'
                color: "white"
                halign: 'center'
                pos_hint: {'center_y': 0.15}
                font_size: 14

            MDFlatButton:
                text: "Registrar-se"
                theme_text_color: "Custom"
                text_color: "blue"
                pos_hint: {'center_x': .5,'center_y': .12}
                font_size: 14
                on_release:
                    app.root.current = "registrar"

        MDFloatLayout:
            MDLabel:
                text: 'Matics'
                font_name: 'Roboto-Bold'
                halign: 'center'
                pos_hint: {'center_x':.5, 'center_y': .8}
                color: "#0C0756"
                font_size: 60

            Image:
                source: 'assets/juros.png'
                size_hint: 1, 1
                pos_hint: {'center_x':.5, 'center_y': .55}

            MDLabel:
                text: 'Desenvolvido por'
                font_name: 'Roboto-Bold'
                color: "#EC5952"
                halign: 'center'
                pos_hint: {'center_x':.5, 'center_y': .185}
                font_size: 20

            MDLabel:
                text: 'Marcelo Soares'
                font_name: 'Roboto-Bold'
                color: "#0C0756"
                halign: 'center'
                pos_hint: {'center_x':.5, 'center_y': .15}
                font_size: 28

            MDIcon:
                icon: "instagram"
                color: "#0C0756"
                halign: 'center'
                pos_hint: {'center_x':.4, 'center_y': .12}
                font_size: 14
            MDLabel:
                text: "s.soares_marcelo"
                color: "#0C0756"
                halign: 'center'
                pos_hint: {'center_x':.52, 'center_y': .12}
                font_size: 14

